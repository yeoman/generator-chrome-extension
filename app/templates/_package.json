    "babel-core": "^5.5.6",<% if (babel) { %>
    "gulp-babel": "^6.1.0",
    "babel-preset-es2015": "^6.1.2",<% } %>
    "del": "^1.1.1",
    "gulp": "^3.9.0",
    "gulp-cache": "^0.2.8",
    "gulp-chrome-manifest": "0.0.11",
    "gulp-eslint": "^0.13.2",
    "gulp-if": "^1.2.5",
    "gulp-imagemin": "^2.2.1",
    "gulp-livereload": "^3.8.1",
    "gulp-load-plugins": "^0.10.0",
    "gulp-minify-css": "^1.2.1",
    "gulp-minify-html": "^1.0.0",<% if (sass) { %>
    "gulp-plumber": "^1.0.1",
    "gulp-sass": "^2.0.0",<% } %>
    "gulp-size": "^1.2.1",
    "gulp-sourcemaps": "^1.5.0",
    "gulp-uglify": "^1.1.0",
    "gulp-useref": "^1.1.1",
    "gulp-zip": "^3.0.2",
